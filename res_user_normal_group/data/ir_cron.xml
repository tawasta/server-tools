<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_add_normal_user_to_menu" model="ir.cron">
            <field name="name">Add Normal user to menu</field>
            <field name="model_id" ref="model_ir_ui_menu" />
            <field name="state">code</field>
            <field name="code">model.add_normal_user_to_menu()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number" eval="1" />
            <field name="interval_type">days</field>
            <field name="numbercall" eval="-1" />
            <field name="priority" eval="5" />
            <field name="doall" eval="0" />
            <field name="active" eval="0" />
        </record>
    </data>
</odoo>
